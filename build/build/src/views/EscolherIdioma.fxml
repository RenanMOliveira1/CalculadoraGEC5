<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="191.0" prefWidth="385.0" styleClass="background" xmlns="http://javafx.com/javafx/8.0.45" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.calculadora.controller.EscolherIdiomaController">
   <children>
      <Button fx:id="btnCancelar" layoutX="266.0" layoutY="140.0" mnemonicParsing="false" onAction="#handleBtnCancelar" prefHeight="31.0" prefWidth="108.0" styleClass="btnEscolherIdioma" text="Canclear" />
      <Button fx:id="btnOk" layoutX="151.0" layoutY="140.0" mnemonicParsing="false" onAction="#handleBtnOk" prefHeight="31.0" prefWidth="108.0" styleClass="btnEscolherIdioma" text="Ok" />
      <ImageView fitHeight="143.0" fitWidth="149.0" layoutX="2.0" layoutY="17.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/idioma-ico.png" />
         </image>
      </ImageView>
      <Label layoutX="153.0" layoutY="60.0" text="Escolher o Idioma Padrão da Aplicação: " />
      <Label layoutX="151.0" layoutY="14.0" text="Idioma Padrão" textFill="#5694ff">
         <font>
            <Font size="29.0" />
         </font>
      </Label>
      <ComboBox fx:id="comboIdiomas" layoutX="151.0" layoutY="93.0" onHidden="#hiddenBtnComboIdiomas" prefHeight="31.0" prefWidth="223.0" promptText="Escolha o Idioma" />
   </children>
   <stylesheets>
      <URL value="@../../../../resources/css/style.css" />
      <URL value="@../css/style.css" />
   </stylesheets>
</AnchorPane>
